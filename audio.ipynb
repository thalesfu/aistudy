   "source": [
    "import librosa\n",
    "\n",
    "# 加载音频文件\n",
    "y, sr = librosa.load('test2.mp3')\n",
    "\n",
    "# 使用 Librosa 进行节拍检测\n",
    "tempo, beat_frames = librosa.beat.beat_track(y=y, sr=sr)\n",
    "\n",
    "# 将节拍帧转换为时间点\n",
    "beat_times = librosa.frames_to_time(beat_frames, sr=sr)\n",
    "\n",
    "# 输出节拍时间点\n",
    "print(\"Detected beat times: \", beat_times)"
   ],
